public class BeanApplyWed {
    private String SubmittingID;
    private String ObjectTypes;
    private String TerminalTypes;
    private String SubmittingPhone;
    private String GroomName;
    private String GroomPhone;
    private String BrideName;
    private String BridePhone;
    private String AreaID;
    private String Address;
    private String WeddingDay;
    private String Budget;
    private String HotelName;
    private String HotelAddress;
    private String TablesNumber;
    private String MealMark;
    private int IsReservationTotel;
    private String Remarks;

    public BeanApplyWed(String submittingID, String objectTypes, String terminalTypes,
                        String submittingPhone, String groomName, String groomPhone, String brideName,
                        String bridePhone, String areaID, String address, String weddingDay,
                        String budget, String hotelName, String hotelAddress, String tablesNumber,
                        String mealMark, int isReservationTotel, String remarks) {
        SubmittingID = submittingID;
        ObjectTypes = objectTypes;
        TerminalTypes = terminalTypes;
        SubmittingPhone = submittingPhone;
        GroomName = groomName;
        GroomPhone = groomPhone;
        BrideName = brideName;
        BridePhone = bridePhone;
        AreaID = areaID;
        Address = address;
        WeddingDay = weddingDay;
        Budget = budget;
        HotelName = hotelName;
        HotelAddress = hotelAddress;
        TablesNumber = tablesNumber;
        MealMark = mealMark;
        IsReservationTotel = isReservationTotel;
        Remarks = remarks;
    }
}
