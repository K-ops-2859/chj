public class PublishDynamicBean {
    private String UserID;
    private String Title;
    private String Content;
    private String FilesId;
    private String CoverImg;
    private int FileType;

    public PublishDynamicBean(int fileType, String userID, String title, String content, String filesId, String coverImg) {
        UserID = userID;
        Title = title;
        Content = content;
        FilesId = filesId;
        CoverImg = coverImg;
        FileType = fileType;
    }
}
