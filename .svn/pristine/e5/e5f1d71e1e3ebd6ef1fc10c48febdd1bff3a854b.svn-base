<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    android:id="@+id/activity_wed_rule"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:background="@color/white"
    tools:context="com.dikai.chenghunjiclient.activity.wedding.WedRuleActivity">
    <View
        android:id="@+id/top_view"
        android:layout_width="match_parent"
        android:layout_height="0dp" />
    <ImageView
        android:id="@+id/back"
        android:layout_width="40dp"
        android:layout_height="60dp"
        android:background="?android:attr/selectableItemBackground"
        android:padding="8dp"
        android:src="@mipmap/ic_app_red_back" />
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="1"
        android:background="@color/gray_background">
        <WebView
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:id="@+id/activity_vip_web"/>
        <ProgressBar
            style="?android:attr/progressBarStyleHorizontal"
            android:layout_width="match_parent"
            android:layout_height="2dp"
            android:id="@+id/activity_vip_progress"
            android:progress="20"
            android:visibility="visible"
            android:layout_alignParentTop="true"/>
    </RelativeLayout>

</LinearLayout>
