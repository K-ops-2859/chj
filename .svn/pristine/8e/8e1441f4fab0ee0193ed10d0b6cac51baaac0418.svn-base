<LinearLayout
    xmlns:myRelativeLayout="http://schemas.android.com/apk/res-auto"
    xmlns:myImageView="http://schemas.android.com/apk/res-auto"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:background="@color/white"
    tools:context="com.dikai.chenghunjiclient.fragment.store.NewHomeFragment">
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="?actionBarSize">
        <LinearLayout
            android:id="@+id/title_layout"
            android:layout_width="match_parent"
            android:layout_height="?actionBarSize"
            android:orientation="horizontal"
            android:background="@color/white">
            <LinearLayout
                android:id="@+id/fragment_store_location_layout"
                android:background="@drawable/button_item_click"
                android:orientation="horizontal"
                android:layout_width="80dp"
                android:gravity="center"
                android:layout_height="match_parent">
                <TextView
                    android:id="@+id/fragment_store_location"
                    android:gravity="center"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:maxWidth="60dp"
                    android:textSize="14sp"
                    android:lines="2"
                    android:ellipsize="end"
                    android:textColor="@color/black_deep"
                    android:text="青岛"/>
                <ImageView
                    android:layout_marginLeft="4dp"
                    android:layout_width="12dp"
                    android:layout_height="12dp"
                    android:src="@mipmap/ic_app_black_down"/>
            </LinearLayout>

            <RelativeLayout
                android:gravity="center"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1">
                <LinearLayout
                    android:id="@+id/fragment_store_search"
                    android:gravity="center_vertical"
                    android:background="@drawable/search_bg"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal">
                    <ImageView
                        android:layout_margin="6dp"
                        android:layout_width="18dp"
                        android:layout_height="18dp"
                        android:src="@mipmap/ic_search"/>
                    <EditText
                        android:visibility="invisible"
                        android:id="@+id/activity_search_project_edit"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:background="@null"
                        android:textColorHint="@color/gray_text"
                        android:textColor="@color/black_light"
                        android:textSize="14sp"
                        android:hint="搜索供应商"/>
                </LinearLayout>
            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/home_scan"
                android:layout_marginStart="20dp"
                android:gravity="center"
                android:background="@drawable/button_item_click"
                android:layout_width="60dp"
                android:layout_height="match_parent">
                <ImageView
                    android:layout_width="20dp"
                    android:layout_height="20dp"
                    android:src="@mipmap/ic_app_new_home_add"
                    android:layout_centerVertical="true"
                    android:layout_centerHorizontal="true"/>
            </RelativeLayout>
        </LinearLayout>
        <android.support.v7.widget.RecyclerView
            android:visibility="gone"
            android:layout_marginStart="8dp"
            android:layout_marginEnd="8dp"
            android:id="@+id/zixun_tab_title"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@color/white"/>
    </RelativeLayout>

    <android.support.v4.widget.SwipeRefreshLayout
        android:id="@+id/my_load_recycler_fresh"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="1">
        <android.support.v4.widget.NestedScrollView
            android:id="@+id/new_store_scroll"
            android:layout_width="match_parent"
            android:layout_height="match_parent">
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical">
                <com.dikai.chenghunjiclient.view.MyRelativeLayout
                    android:background="@color/white"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    myRelativeLayout:relative_ratio="2.9">
                    <com.zhouwei.mzbanner.MZBannerView
                        android:id="@+id/fragment_head_ad"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        app:open_mz_mode="true"
                        app:middle_page_cover="false"
                        app:canLoop="true"
                        app:indicatorAlign="center"/>
                </com.dikai.chenghunjiclient.view.MyRelativeLayout>
                <com.bigkoo.convenientbanner.ConvenientBanner
                    android:id="@+id/fragment_head_convenientBanner"
                    android:layout_width="match_parent"
                    android:layout_height="190dp"
                    app:canLoop="true" />
                <com.dikai.chenghunjiclient.view.MyImageView
                    android:layout_marginTop="4dp"
                    android:id="@+id/img_info_1"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@color/white"
                    android:scaleType="fitXY"
                    android:src="@color/white"
                    myImageView:aspect_ratio="3"/>
                <!--<android.support.v7.widget.CardView-->
                    <!--android:layout_marginStart="16dp"-->
                    <!--android:layout_marginEnd="16dp"-->
                    <!--android:layout_width="match_parent"-->
                    <!--android:layout_height="wrap_content"-->
                    <!--tools:targetApi="lollipop"-->
                    <!--app:cardCornerRadius="5dp"-->
                    <!--app:cardUseCompatPadding="true"-->
                    <!--app:cardPreventCornerOverlap="false">-->
                <!--</android.support.v7.widget.CardView>-->
                <LinearLayout
                    android:layout_marginTop="24dp"
                    android:layout_marginEnd="18dp"
                    android:layout_marginStart="18dp"
                    android:layout_marginBottom="12dp"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:gravity="center_vertical">

                    <TextView
                        android:layout_width="0dp"
                        android:layout_weight="1"
                        android:layout_height="wrap_content"
                        android:textColor="@color/black"
                        android:textSize="20sp"
                        android:textStyle="bold"
                        android:text="热门活动"
                        android:id="@+id/textView5"/>

                    <TextView
                        android:id="@+id/all_ad"
                        android:clickable="true"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textColor="@color/gray_text"
                        android:textSize="13sp"
                        android:text="查看更多"/>
                </LinearLayout>
                <com.dikai.chenghunjiclient.view.MyRelativeLayout
                    android:layout_marginLeft="18dp"
                    android:layout_marginRight="18dp"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    myRelativeLayout:relative_ratio="3">
                    <com.joooonho.SelectableRoundedImageView
                        android:id="@+id/img_info_4"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:scaleType="centerCrop"
                        app:sriv_left_top_corner_radius="4dp"
                        app:sriv_right_top_corner_radius="4dp"
                        app:sriv_left_bottom_corner_radius="4dp"
                        app:sriv_right_bottom_corner_radius="4dp"
                        app:sriv_border_width="0dip"
                        app:sriv_border_color="#008fea"
                        app:sriv_oval="false"
                        android:layout_alignParentTop="true"
                        android:layout_alignParentStart="true"/>
                </com.dikai.chenghunjiclient.view.MyRelativeLayout>
                <LinearLayout
                    android:layout_marginTop="5dp"
                    android:paddingStart="18dp"
                    android:paddingEnd="18dp"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal">
                    <LinearLayout
                        android:layout_width="0dp"
                        android:layout_weight="1"
                        android:layout_height="wrap_content"
                        android:orientation="vertical"
                        android:gravity="center">
                        <!--<TextView-->
                            <!--android:id="@+id/ad_text_1"-->
                            <!--android:lines="1"-->
                            <!--android:ellipsize="end"-->
                            <!--android:layout_width="wrap_content"-->
                            <!--android:layout_height="wrap_content"-->
                            <!--android:textColor="@color/black"-->
                            <!--android:textSize="14sp"-->
                            <!--android:text="0元办婚礼"-->
                            <!--android:textStyle="bold"/>-->
                        <!--<TextView-->
                            <!--android:id="@+id/ad_info_1"-->
                            <!--android:lines="1"-->
                            <!--android:ellipsize="end"-->
                            <!--android:layout_marginTop="4dp"-->
                            <!--android:layout_width="wrap_content"-->
                            <!--android:layout_height="wrap_content"-->
                            <!--android:textColor="@color/gray_text"-->
                            <!--android:textSize="11sp"-->
                            <!--android:text="花多少返多少"/>-->
                        <com.dikai.chenghunjiclient.view.MyImageView
                            android:id="@+id/ad_img_1"
                            android:layout_marginTop="4dp"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:background="@color/gray_background"
                            android:scaleType="centerCrop"
                            myImageView:aspect_ratio="0.83"/>
                    </LinearLayout>

                    <LinearLayout
                        android:layout_marginStart="9dp"
                        android:layout_marginEnd="9dp"
                        android:layout_width="0dp"
                        android:layout_weight="1"
                        android:layout_height="wrap_content"
                        android:orientation="vertical"
                        android:gravity="center">
                        <!--<TextView-->
                            <!--android:id="@+id/ad_text_2"-->
                            <!--android:lines="1"-->
                            <!--android:ellipsize="end"-->
                            <!--android:layout_width="wrap_content"-->
                            <!--android:layout_height="wrap_content"-->
                            <!--android:textColor="@color/black"-->
                            <!--android:textSize="14sp"-->
                            <!--android:text="婚嫁采购节"-->
                            <!--android:textStyle="bold"/>-->
                        <!--<TextView-->
                            <!--android:id="@+id/ad_info_2"-->
                            <!--android:lines="1"-->
                            <!--android:ellipsize="end"-->
                            <!--android:layout_marginTop="4dp"-->
                            <!--android:layout_width="wrap_content"-->
                            <!--android:layout_height="wrap_content"-->
                            <!--android:textColor="@color/gray_text"-->
                            <!--android:textSize="11sp"-->
                            <!--android:text="一站式大采购"/>-->
                        <com.dikai.chenghunjiclient.view.MyImageView
                            android:id="@+id/ad_img_2"
                            android:layout_marginTop="4dp"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:background="@color/gray_background"
                            android:scaleType="centerCrop"
                            myImageView:aspect_ratio="0.83"/>
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="0dp"
                        android:layout_weight="1"
                        android:layout_height="wrap_content"
                        android:orientation="vertical"
                        android:gravity="center">
                        <!--<TextView-->
                            <!--android:id="@+id/ad_text_3"-->
                            <!--android:lines="1"-->
                            <!--android:ellipsize="end"-->
                            <!--android:layout_width="wrap_content"-->
                            <!--android:layout_height="wrap_content"-->
                            <!--android:textColor="@color/black"-->
                            <!--android:textSize="14sp"-->
                            <!--android:text="邀请新人"-->
                            <!--android:textStyle="bold"/>-->
                        <!--<TextView-->
                            <!--android:id="@+id/ad_info_3"-->
                            <!--android:lines="1"-->
                            <!--android:ellipsize="end"-->
                            <!--android:layout_marginTop="4dp"-->
                            <!--android:layout_width="wrap_content"-->
                            <!--android:layout_height="wrap_content"-->
                            <!--android:textColor="@color/gray_text"-->
                            <!--android:textSize="11sp"-->
                            <!--android:text="100元现金赚不停"/>-->
                        <com.dikai.chenghunjiclient.view.MyImageView
                            android:id="@+id/ad_img_3"
                            android:layout_marginTop="4dp"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:background="@color/gray_background"
                            android:scaleType="centerCrop"
                            myImageView:aspect_ratio="0.83"/>
                    </LinearLayout>
                </LinearLayout>
                <LinearLayout
                    android:layout_marginTop="24dp"
                    android:layout_marginEnd="18dp"
                    android:layout_marginStart="18dp"
                    android:layout_marginBottom="12dp"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:gravity="center_vertical">

                    <TextView
                        android:layout_width="0dp"
                        android:layout_weight="1"
                        android:layout_height="wrap_content"
                        android:textColor="@color/black"
                        android:textSize="20sp"
                        android:textStyle="bold"
                        android:text="精选套餐"/>

                    <TextView
                        android:id="@+id/all_project"
                        android:clickable="true"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textColor="@color/gray_text"
                        android:textSize="13sp"
                        android:text="查看更多"/>
                </LinearLayout>
                <android.support.v7.widget.RecyclerView
                    android:layout_marginStart="18dp"
                    android:id="@+id/hot_project_recycler"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@color/white"/>
                <TextView
                    android:layout_marginTop="24dp"
                    android:layout_marginEnd="18dp"
                    android:layout_marginStart="18dp"
                    android:layout_marginBottom="12dp"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textColor="@color/black"
                    android:textSize="20sp"
                    android:textStyle="bold"
                    android:text="备婚工具"/>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal">
                    <LinearLayout
                        android:paddingTop="16dp"
                        android:paddingBottom="16dp"
                        android:paddingStart="18dp"
                        android:id="@+id/free_project"
                        android:layout_width="0dp"
                        android:layout_weight="1"
                        android:layout_height="wrap_content"
                        android:gravity="center_vertical"
                        android:orientation="horizontal">
                        <ImageView
                            android:layout_width="36dp"
                            android:layout_height="36dp"
                            android:src="@mipmap/home_gongju1"/>
                        <LinearLayout
                            android:layout_marginStart="10dp"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:orientation="vertical">
                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:textColor="@color/black"
                                android:textSize="15sp"
                                android:textStyle="bold"
                                android:text="免费领对戒"/>
                            <TextView
                                android:layout_marginTop="8dp"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:textColor="@color/gray_text"
                                android:textSize="11sp"
                                android:text="见证美好爱情"/>
                        </LinearLayout>
                    </LinearLayout>

                    <View
                        android:layout_width="1dp"
                        android:layout_height="75dp"
                        android:background="@color/gray_btn3"/>
                    <LinearLayout
                        android:paddingTop="16dp"
                        android:paddingBottom="16dp"
                        android:paddingStart="18dp"
                        android:gravity="center_vertical"
                        android:id="@+id/free_wedding"
                        android:layout_width="0dp"
                        android:layout_weight="1"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal">
                        <ImageView
                            android:layout_width="36dp"
                            android:layout_height="36dp"
                            android:src="@mipmap/home_gongju2"/>
                        <LinearLayout
                            android:layout_marginStart="10dp"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:orientation="vertical">
                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:textColor="@color/black"
                                android:textSize="15sp"
                                android:textStyle="bold"
                                android:text="免费办婚礼"/>
                            <TextView
                                android:layout_marginTop="10dp"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:textColor="@color/gray_text"
                                android:textSize="11sp"
                                android:text="再不预约就晚啦"/>
                        </LinearLayout>
                    </LinearLayout>
                </LinearLayout>
                <View
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:background="@color/gray_btn3"/>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal">
                    <LinearLayout
                        android:paddingTop="16dp"
                        android:paddingBottom="16dp"
                        android:paddingStart="18dp"
                        android:gravity="center_vertical"
                        android:id="@+id/wedding_danbao"
                        android:layout_width="0dp"
                        android:layout_weight="1"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal">
                        <ImageView
                            android:layout_width="36dp"
                            android:layout_height="36dp"
                            android:src="@mipmap/home_gongju3"/>
                        <LinearLayout
                            android:layout_marginStart="10dp"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:orientation="vertical">
                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:textColor="@color/black"
                                android:textSize="15sp"
                                android:textStyle="bold"
                                android:text="婚礼担保"/>
                            <TextView
                                android:layout_marginTop="10dp"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:textColor="@color/gray_text"
                                android:textSize="10sp"
                                android:text="完美婚礼由此呈现"/>
                        </LinearLayout>
                    </LinearLayout>
                    <View
                        android:layout_width="1dp"
                        android:layout_height="73dp"
                        android:background="@color/gray_btn3"/>
                    <LinearLayout
                        android:paddingTop="16dp"
                        android:paddingBottom="16dp"
                        android:paddingStart="18dp"
                        android:gravity="center_vertical"
                        android:id="@+id/share_project"
                        android:layout_width="0dp"
                        android:layout_weight="1"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal">
                        <ImageView
                            android:layout_width="36dp"
                            android:layout_height="36dp"
                            android:src="@mipmap/home_gongju4"/>
                        <LinearLayout
                            android:layout_marginStart="10dp"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:orientation="vertical">
                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:textColor="@color/black"
                                android:textSize="15sp"
                                android:textStyle="bold"
                                android:text="共享方案"/>
                            <TextView
                                android:layout_marginTop="10dp"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:textColor="@color/gray_text"
                                android:textSize="10sp"
                                android:text="左手创意右手灵感"/>
                        </LinearLayout>
                    </LinearLayout>
                </LinearLayout>
                <TextView
                    android:layout_marginTop="24dp"
                    android:layout_marginEnd="18dp"
                    android:layout_marginStart="18dp"
                    android:layout_marginBottom="12dp"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textColor="@color/black"
                    android:textSize="20sp"
                    android:textStyle="bold"
                    android:text="备婚笔记"/>
                <android.support.v7.widget.RecyclerView
                    android:layout_marginStart="10dp"
                    android:id="@+id/zixun_tab"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@color/white"/>
                <android.support.v7.widget.RecyclerView
                    android:id="@+id/zixun_list"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@color/white"/>
            </LinearLayout>
        </android.support.v4.widget.NestedScrollView>
    </android.support.v4.widget.SwipeRefreshLayout>
</LinearLayout>
