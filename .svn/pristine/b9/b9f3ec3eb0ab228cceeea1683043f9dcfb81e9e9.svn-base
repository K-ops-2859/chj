<?xml version="1.0" encoding="utf-8"?>
<com.dikai.chenghunjiclient.view.MyRelativeLayout
    xmlns:myRelativeLayout="http://schemas.android.com/apk/res-auto"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_margin="5dp"
    myRelativeLayout:relative_ratio="1">
    <com.joooonho.SelectableRoundedImageView
        android:id="@+id/item_publish_circle_img"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:scaleType="centerCrop"
        android:src="@mipmap/ic_app_upload_place"
        app:sriv_left_top_corner_radius="4dp"
        app:sriv_right_top_corner_radius="4dp"
        app:sriv_left_bottom_corner_radius="4dp"
        app:sriv_right_bottom_corner_radius="4dp"
        app:sriv_border_width="0dip"
        app:sriv_border_color="#008fea"
        app:sriv_oval="false"/>
    <ImageView
        android:id="@+id/item_publish_circle_close"
        android:layout_width="22dp"
        android:layout_height="22dp"
        android:src="@mipmap/ic_app_close_img"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"/>

</com.dikai.chenghunjiclient.view.MyRelativeLayout>